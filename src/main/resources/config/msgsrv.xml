<?xml version='1.0' encoding='UTF-8'?>
<config>
	
	<!-- 
		host		:[必填]MsgSrv的地址
		port		:[必填]MsgSrv连接端口号
		appname		:[必填]MsgSrv登录名
		username	:[必填]登录MsgSrv的用户名
		password	:[必填]登录MsgSrv的密码，改值需要yhbf加密
		selector	:[可选]MsgSrv连接器等待数据超时时间，单位ms，默认1000ms
		pulse		:[可选]MsgSrv连接器空闲时间，单位ms, 默认：60000ms
		reconnect	:[可选]MsgSrv尝试重连时间间隔，单位ms, 默认：5000ms
	 -->
	<msgsrv 
		host="192.168.16.222"	
		port="6004"
		appname="ParkSearch"
		username="ParkSearch"
		password="ParkSearch"
		selector="1000"
		pulse="1200000"
		reconnect="1000" />
	
</config>